 <h1>{{title}}  <i class="fa-solid fa-list"></i></h1>                                          

  


 <table class="table table-dark table-striped"</table>
        <thread>
            <tr>
                 <th>Full Name</th>
                 <th>Email</th>
                 <th>Contact</th>
                 <th>City</th>
                 <td>  </td>
            </tr>
        </thread>

      <tbody>
             {{#each list}}
             <tr>
                  <td>{{this.fullname}}</td>
                  <td>{{this.email}}</td>
                  <td>{{this.contact}}</td>
                  <td>{{this.city}}</td>
                  <td>
                   <a href="/{{this._id}}" <i class="fa-solid fa-pencil"></i></a>
                   <a href="/delete/{{this._id}}" <i class="fa-solid fa-trash-can"></i></a>
                  </td>
             </tr>
              {{/each}}
      </tbody>
 </table>
         

    <a class="btn btn-dark" href="/"> <i class="fa-solid fa-circle-plus fa-beat"></i> Create New</a>      

